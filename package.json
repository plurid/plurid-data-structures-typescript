    "homepage": "https://github.com/plurid/plurid-data-structures-typescript",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "types": "distribution/index.d.ts",
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "scripts": {
        "clean": "rm -rf ./distribution",
        "lint": "eslint ./source/index.ts",
        "test": "jest -c ./configurations/jest.config.js --coverage ./source",
        "test.source": "jest -c ./configurations/jest.config.js",
        "build.removeTests": "rm -rf `find ./distribution/ -type d -name __tests__`",
        "start": "yarn clean && rollup -c -w",
        "build.development": "yarn clean && rollup -c",
        "build.production": "rollup -c --environment BUILD:production",
        "build": "yarn clean && yarn lint && yarn test && yarn build.production && yarn build.removeTests",
        "prepublishOnly": "yarn build"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.2",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.6",
